{
  "name": "desmodder",
  "description": "Supercharge your Desmos graph creation and sharing experience with many convenient features",
  "version": "0.8.5",
  "author": {
    "name": "Jared Hughes"
  },
  "browserslist": "defaults",
  "eslintIgnore": [
    "dist/*.js"
  ],
  "scripts": {
    "prettier": "prettier --write .",
    "eslint-fix": "eslint --fix  \"src/**/*.ts\"",
    "build": "webpack --mode production --env=browser=chrome",
    "build-ff": "webpack --mode production --env=browser=firefox",
    "dev": "webpack --mode development --env=browser=chrome",
    "dev-ff": "webpack --mode development --env=browser=firefox",
    "lint": "prettier-check . && tsc --noEmit && eslint \"src/**/*.ts\"",
    "test": "npm run test:jest",
    "test:jest": "jest --config jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DesModder/DesModder"
  },
  "devDependencies": {
    "@desmodder/graph-state": "^0.2.3",
    "@lezer/generator": "^1.1.1",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/babel__core": "^7.1.19",
    "@types/babel__standalone": "^7.1.4",
    "@types/babel__template": "^7.4.1",
    "@types/chrome": "0.0.197",
    "@types/jest": "^28.1.4",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "^5.2.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "less": "^4.1.1",
    "less-loader": "^8.1.0",
    "lezer-loader": "^0.3.0",
    "prettier-check": "^2.0.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/standalone": "^7.19.2",
    "@babel/template": "^7.18.10",
    "@codemirror/autocomplete": "^6.2.0",
    "@codemirror/commands": "^6.1.0",
    "@codemirror/language": "^6.2.1",
    "@codemirror/lint": "^6.0.0",
    "@codemirror/search": "^6.2.0",
    "@codemirror/state": "^6.1.1",
    "@codemirror/view": "^6.2.4",
    "@ffmpeg/core": "^0.10.0",
    "@ffmpeg/ffmpeg": "^0.10.0",
    "@fluent/bundle": "^0.17.1",
    "@lezer/common": "^1.0.1",
    "@lezer/lr": "^1.2.3",
    "@types/desmos": "^1.6.0",
    "prettier": "^2.7.1",
    "process": "^0.11.10"
  }
