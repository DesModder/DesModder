{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "strictNullChecks": true,
    "module": "es6",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "jsxFactory": "jsx",
    "target": "es2020",
    "allowJs": true,
    "strict": true,
    "baseUrl": "src",
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": ["./src/"]
}
